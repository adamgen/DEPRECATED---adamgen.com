{
    "name": "next",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "is-ci-cli serve dev",
        "serve": "next start",
        "lint": "next lint",
        "prettier": "prettier --config .prettierrc.json './**/{*,\\[*\\]}.{js,ts,tsx,css,md,json}'",
        "format": "npm run prettier -- --write",
        "validate:check": "npm run prettier -- --check",
        "gql-get-schema": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema.gql",
        "gql-codegen": "apollo client:codegen --target=typescript --localSchemaFile=schema.gql --tagName=gql --includes=lib/query.gql --tsFileExtension=d.ts"
    },
    "dependencies": {
        "@svgr/webpack": "^5.5.0",
        "classnames": "^2.3.1",
        "date-fns": "^2.28.0",
        "eslint-config-prettier": "^8.3.0",
        "is-ci-cli": "^2.2.0",
        "next": "11.1.2",
        "raw-loader": "^4.0.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "remark": "^14.0.2",
        "remark-html": "^15.0.1",
        "rimraf": "^3.0.2",
        "sharp": "^0.29.1",
        "styled-components": "^5.3.1",
        "zx": "^4.2.0"
    },
    "devDependencies": {
        "@types/react": "17.0.20",
        "@types/styled-components": "^5.1.14",
        "apollo": "2.33.4",
        "apollo-codegen-core": "0.40.3",
        "apollo-language-server": "1.26.3",
        "autoprefixer": "^10.3.4",
        "eslint": "7.32.0",
        "eslint-config-next": "11.1.2",
        "graphql-tag": "^2.12.6",
        "postcss": "^8.3.6",
        "prettier": "^2.4.0",
        "tailwindcss": "^3.0.7",
        "typescript": "4.4.3"
    }
}
